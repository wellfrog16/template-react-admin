{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-airbnb"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "ter-indent": [true, 4],
    "indent": [true, "spaces", 4],
    "ordered-imports": false, // import顺序
    "import-name": [0], // import的变量名
    "no-console": [true, "warn"]
  }
}
